    (function() {
    let initialized = false;
    
    function initImages() {
        if (initialized) return;
        initialized = true;
        
        // Remove all listeners
        ['click', 'touchstart', 'keydown', 'mousemove'].forEach(event => {
            document.removeEventListener(event, initImages);
        });

        // Load all images
        document.querySelectorAll('.lazy-load-image').forEach(picture => {
            picture.querySelectorAll('source').forEach(source => {
                source.srcset = source.dataset.srcset;
            });
            picture.querySelectorAll('img').forEach(img => {
                img.src = img.dataset.src;
            });
        });
    }

    // Add event listeners for user interaction
    ['click', 'touchstart', 'keydown', 'mousemove'].forEach(event => {
        document.addEventListener(event, initImages, { passive: true });
    });

    // Fallback after 30s
    setTimeout(initImages, 30000);
})(); 
</script>
